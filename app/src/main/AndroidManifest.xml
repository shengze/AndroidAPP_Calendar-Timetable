<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shengze3322vs.year">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/calendar_theme">
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity_TimeTable"
            android:label="@string/title_activity_main__time_table" />
        <activity
            android:name=".MainActivity_AddEvents"
            android:label="@string/title_activity_main__add_events" />
        <activity
            android:name=".MainActivity_UpdateEvents"
            android:label="@string/title_activity_main__add_events" />
        <activity
            android:name=".MainActivity_DayEventList"
            android:label="@string/title_activity_main__add_events" />

        <service
            android:name=".MyIntentService"
            android:exported="false"></service>
    </application>

</manifest>